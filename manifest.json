{
  "manifest_version": 2,
  "name": "SetTimeout",
  "description": "This extension help you timing open web page",
  "version": "0.1.0",
  "author": "轻键快码",
  "automation": "...",
  "background": {
    "scripts": [
      "scripts/background.js",
      "scripts/devtools-page.js"
    ]
  },
  "devtools_page": "html/devtools-page.html",
  // "content_scripts": [
  //   {
  //     "js": ["js/"]
  //   }
  // ],
  "icons": {
    "16": "images/logo.png"
  },
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_title": "Settimeout",
    "default_popup": "html/popup.html"
  },
  "page_action": {
    "default_icon": "images/logo.png",
    "default_title": "Settimeout",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "cookies",
    "http://*/*",
    "management",
    "tabs",
    "contextMenus"
  ]
}